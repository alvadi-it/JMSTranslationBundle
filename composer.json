{
    "name": "alvadi-it/translation-bundle",
    "type": "symfony-bundle",
    "description": "Puts the Symfony Translation Component on steroids",
    "keywords": [
        "translation",
        "multilanguage",
        "extract",
        "webinterface",
        "ui",
        "i18n",
        "extraction",
        "interface"
    ],
    "homepage": "http://jmsyst.com/bundles/JMSTranslationBundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "nikic/php-parser": "^4.9 || ^5",
        "symfony/console": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/config": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/translation-contracts": "^3.0",
        "symfony/validator": "^7.0",
        "twig/twig": "^3.0",
        "psr/log": "^1.0 || ^2.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.11",
        "doctrine/coding-standard": "^8.2.1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.13",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "nyholm/nsa": "^1.0.1",
        "symfony/phpunit-bridge": "^7.0",
        "sensio/framework-extra-bundle": "^7.0",
        "symfony/asset": "^7.0",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/form": "^7.0",
        "symfony/security-csrf": "^7.0",
        "symfony/templating": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/flex": "^2.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "symfony": {
            "allow-contrib": true
        }
    },
    "autoload": {
        "psr-4": {
            "JMS\\TranslationBundle\\": ""
        }
    },
    "minimum-stability": "stable"
}
